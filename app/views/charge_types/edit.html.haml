- @title = "Edit Charge Type - #{@charge_type.name_was}"

- content_for :header, @charge_type.name_was
= render "internal/tabs"
- content_for :breadcrumbs do
  %ol
    %li= link_to "charge types", charge_types_path
    %li= link_to @charge_type.name_was.to_s.downcase, @charge_type
    %li.breadcrumb-muted edit
- content_for :sidebar, render("internal/sidebar_setup")

.dashboard-container= render "charge_types/form", charge_type: @charge_type
