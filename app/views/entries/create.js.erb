showEntryContents();
$('#new-entry-dialog').modal('hide');
$('#new_entry')[0].reset();
$('.has-error').removeClass('has-error');

<% @entries = current_user.entries.with_date_for_calendar(@entry.billing_date, @entry.billing_date) %>
$("[data-date='<%= @entry.billing_date.strftime("%m/%d/%Y") %>']").html("<%=j render 'entries/calendar_day_body', iDate: @entry.billing_date %>");

<% @start_month = @entry.billing_date.beginning_of_month %>
<% @end_month = @entry.billing_date.end_of_month %>
$("#month-spending").html("<%=j render 'accounts/month_spending' %>")
$("#month-income-loss").html("<%=j render 'entries/month_income_loss' %>")

activateEntryDraggables();
tooltipsReady();
