- @title = "Edit Account - #{@account.name_was}"

- content_for :header, @account.name_was
= render "internal/tabs"
- content_for :breadcrumbs do
  %ol
    %li= link_to "accounts", accounts_path
    %li= link_to @account.name_was.to_s.downcase, @account
    %li.breadcrumb-muted edit
- content_for :sidebar, render("internal/sidebar_setup")

.dashboard-container= render "accounts/form", account: @account
