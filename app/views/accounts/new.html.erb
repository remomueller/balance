<% @title = 'New Account' %>
<div class="title">
  <%= @title %>
  <%= link_to 'view all', accounts_path %>
</div>
<div class="content">
  <div class="form">
    <%= form_for @account do |f| %>
      <%= render :partial => 'form' %>
      <div class="submit"><%= f.submit 'Create', :disable_with => 'Creating...' %> or <%= cancel %></div>
    <% end %>
  </div>
</div>